{
    "routes": [
      {
        "src": "/(events|profile|recruitments|clubs_assocs|clubs_assocs/:id)",
        "dest": "/index.html"
      },
      {
        "src": "/",
        "dest": "/index.html"
      }
    ],
    "rewrites": [
      {
        "source": "/clubs_assocs/(.*)",
        "destination": "/clubs_assocs/$1"
      },
      {
        "source": "/(.*)",
        "destination": "/index.html"
      }
    ]
  }
  